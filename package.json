{
	"name": "ijan",
	"version": "1.0.0",
	"scripts": {
		"killall": "killall -9 node",
		"dev": "NODE_ENV='dev' cross-env NODE_OPTIONS='--inspect' next",
		"build": "rm -rf ./build && next build",
		"next:build": "next build",
		"start": "next start -p $PORT",
		"deploy": "NODE_ENV='production' && yarn build && gcloud app deploy",
		"export": "next export",
		"tsc": "tsc --pretty --noEmit",
		"format": "prettier --write .",
		"lint": "eslint . --ext ts --ext tsx --ext js"
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn format"
		]
	},
	"dependencies": {
		"cross-env": "^7.0.3",
		"firebase": "^8.2.8",
		"firebase-admin": "^9.5.0",
		"js-cookie": "^2.2.1",
		"lodash": "^4.17.20",
		"mobx": "^6.1.7",
		"mobx-react": "^7.1.0",
		"next": "10.0.7",
		"next-images": "^1.7.0",
		"node-fetch": "^2.6.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-images-uploading": "^3.1.2",
		"react-is": "^17.0.1",
		"react-rnd": "^10.2.4",
		"react-scripts": "^4.0.1",
		"styled-components": "^5.2.1",
		"swr": "^0.4.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.17",
		"@testing-library/react": "^11.2.3",
		"@types/js-cookie": "^2.2.6",
		"@types/lodash": "^4.14.168",
		"@types/node": "^14.14.30",
		"@types/node-fetch": "^2.5.8",
		"@types/prettier": "^2.2.1",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@types/react-rnd": "^8.0.0",
		"@types/styled-components": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"babel-plugin-styled-components": "^1.12.0",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-react": "^7.19.0",
		"identity-obj-proxy": "^3.0.0",
		"lint-staged": "^10.0.10",
		"prettier": "^2.0.2",
		"typescript": "^4.1.5"
	}
}
